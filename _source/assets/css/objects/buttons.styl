.button
	margin-top 42px
	border-radius 5px
	border none
	background-color $green-one
	color $white
	box-shadow 0px 2px 5px rgba(0, 0, 0, .2)
	padding 12px 32px
	text-transform capitalize

.button:hover
	cursor pointer
	color $gray-two

.button-white
	background-color $white
	border 2px solid $green-one
	padding 10px 30px
	color $green-one
